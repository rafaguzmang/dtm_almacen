<?xml version="1.0" encoding="utf-8"?>
<odoo>

   <data>
    <record id="dtm_almacen_salidas_view_action" model="ir.actions.act_window">
        <field name="name">Historial Salidas</field>
        <field name="res_model">dtm.almacen.salidas</field>
        <field name="view_mode">tree</field>            
    </record>

    <record id="dtm_almacen_salidas_view_tree" model="ir.ui.view">
        <field name="name">dtm.almacen.salidas.tree</field>
        <field name="model">dtm.almacen.salidas</field>
        <field name="arch" type="xml">
            <tree >
                <field name="id"/>
                <field name="codigo_material"/>
                <field name="cantidad_salida"/>
                <field name="responsable"/>
                <field name="fecha_salida"/>
            </tree>
        </field>
    </record>

    <record id = "dtm_almacen_salidas_search" model="ir.ui.view">
            <field name="name">dtm_almacen_salidas_search</field>
            <field name="model">dtm.almacen.salidas</field>
            <field name ="arch" type="xml">
               <search>
                    <field name="codigo_material"/>
               </search>
            </field>
    </record>

<!--    <menuitem id="dtm_almacen_salidas_view_menu" action="dtm_almacen_salidas_view_action" />-->
   </data>
   

</odoo>
